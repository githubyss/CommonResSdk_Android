<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false"
          android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/comres_buttonToggleBgUnchecked_gray_blue" />
            <corners android:radius="@dimen/comres_buttonToggleCornerRadius_default" />
            <stroke android:width="@dimen/comres_buttonToggleStrokeWidth_default"
                    android:color="@color/comres_buttonToggleStroke_gray_blue" />
        </shape>
    </item>

    <item android:state_checked="true"
          android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/comres_buttonToggleBgChecked_gray_blue" />
            <corners android:radius="@dimen/comres_buttonToggleCornerRadius_default" />
            <stroke android:width="@dimen/comres_buttonToggleStrokeWidth_default"
                    android:color="@color/comres_buttonToggleStroke_gray_blue" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/comres_buttonToggleBgDisabled_gray_blue" />
            <corners android:radius="@dimen/comres_buttonToggleCornerRadius_default" />
            <stroke android:width="@dimen/comres_buttonToggleStrokeWidth_default"
                    android:color="@color/comres_buttonToggleStroke_gray_blue" />
        </shape>
    </item>
</selector>
