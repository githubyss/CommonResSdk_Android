<?xml version="1.0" encoding="utf-8"?>
<shape
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:shape="rectangle"
        >

    <gradient
            android:angle="90"
            android:centerY="0.5"
            android:startColor="@color/comres_itemCardBgStart_orange"
            android:endColor="@color/comres_itemCardBgEnd_orange"
            android:type="linear"
            android:useLevel="false"
            />
    <corners android:radius="@dimen/comres_itemCardCornerRadius_default" />
    <stroke
            android:width="@dimen/comres_itemCardStrokeWidth_default"
            android:color="@color/comres_itemCardStroke_orange"
            />
</shape>
