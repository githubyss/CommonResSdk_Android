<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Button checkable rectangle gray-blue style. -->
    <style name="comres_button_checkable_rectangle_gray_blue">
        <item name="android:background">@drawable/comres_button_checkable_bg_gray_blue_rectangle_micro_all</item>
        <item name="android:button">@null</item>
        <item name="android:textColor">@drawable/comres_button_checkable_bg_gray_blue_rectangle_micro_all</item>
        <item name="android:textSize">@dimen/comres_button_checkable_text_size</item>
        <item name="android:enabled">true</item>
        <item name="android:checkable">true</item>
        <item name="android:checked">false</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <!-- Button checkable rectangle blue-transparent style. -->
    <style name="comres_button_checkable_rectangle_blue_transparent">
        <item name="android:background">@drawable/comres_button_checkable_bg_blue_transparent_rectangle_micro_all</item>
        <item name="android:button">@null</item>
        <item name="android:textColor">@drawable/comres_button_checkable_bg_blue_transparent_rectangle_micro_all</item>
        <item name="android:textSize">@dimen/comres_button_checkable_text_size</item>
        <item name="android:enabled">true</item>
        <item name="android:checkable">true</item>
        <item name="android:checked">false</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
    </style>
</resources>
