<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:color="@color/comres_button_checkable_text_normal_gray_blue"
          android:state_checked="false"
          android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/comres_button_checkable_bg_normal_gray_blue" />
            <corners android:radius="@dimen/comres_cornerRadius_micro" />
            <stroke android:color="@color/comres_button_checkable_stroke_gray_blue"
                    android:width="@dimen/comres_strokeWidth_none" />
        </shape>
    </item>

    <item android:color="@color/comres_button_checkable_text_checked_gray_blue"
          android:state_checked="true"
          android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/comres_button_checkable_bg_checked_gray_blue" />
            <corners android:radius="@dimen/comres_cornerRadius_micro" />
            <stroke android:color="@color/comres_button_checkable_stroke_gray_blue"
                    android:width="@dimen/comres_strokeWidth_none" />
        </shape>
    </item>

    <item android:color="@color/comres_button_checkable_text_disabled_gray_blue"
          android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/comres_button_checkable_bg_disabled_gray_blue" />
            <corners android:radius="@dimen/comres_cornerRadius_micro" />
            <stroke android:color="@color/comres_button_checkable_stroke_gray_blue"
                    android:width="@dimen/comres_strokeWidth_none" />
        </shape>
    </item>
</selector>
